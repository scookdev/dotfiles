[color]
	ui = true
[user]
	name = binarymason
	email = mace2345@gmail.com
[core]
	editor = vim
	autocrlf = input
[push]
	default = simple
[alias]
	s = status
	p = push
	l = log --oneline --graph --decorate --all
	cm = commit -m
	co = checkout
        br = branch
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        type = cat-file -t
        dump = cat-file -p


[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
